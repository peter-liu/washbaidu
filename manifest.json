{
    "name": "Washbaidu",
    "description": "优化百度搜索界面，移除过度广告，提升百度体验。",
    "version": "1.1",
    "manifest_version": 3,
    "web_accessible_resources": [
        {
            "resources": [
                "searchlogo.png"
            ],
            "matches": [
                "https://www.baidu.com/*"
            ]
        }
    ],
    "icons": { 
	    "16" : "icons/logo16.png",
	    "32" : "icons/logo32.png",
        "48" : "icons/logo48.png",
        "64" : "icons/logo64.png",
        "128": "icons/logo128.png" 
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "WashBaidu"
    },
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://www.baidu.com/"
    ]
}
